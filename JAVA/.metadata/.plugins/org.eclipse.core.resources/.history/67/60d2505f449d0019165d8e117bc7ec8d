package test;

public class Coin {
	public static void main(String[] args) {
		//연산자 활용과 변수의 이해 500 100 50 10
		final int money=2680;						//상수	
		int rest_money= money;					//변수의 초기화
		int count;										//변수의 선언
		
		
		System.out.println("500원 : " + money/500);
		System.out.println("남은 금액 : " + rest_money%500);
		System.out.println("100원 : " + rest_money);
	}
}
